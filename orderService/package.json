{
  "name": "orderService",
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/app.js",
    "dev": "concurrently \"yarn tsc --watch\" \"nodemon -w src -e ts --exec ts-node src/app.ts\""
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.1",
    "nodemon": "^3.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
